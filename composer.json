{
    "name": "eureka2/g6k",
    "version": "3.4.3",
    "license": "MIT",
    "type": "project",
    "description": "Generator of simulator of calculation (calculator)",
    "authors": [
        {
            "name": "Eureka2",
            "homepage": "https://github.com/eureka2/G6K"
        }
    ],
    "autoload": {
        "psr-4": {
            "EUREKA\\G6KBundle\\": "src/EUREKA/G6KBundle"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=5.5.9",
        "ashleydawson/simple-pagination": "1.0.*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "flow/jsonpath": "^0.4.0",
        "friendsofsymfony/user-bundle": "~2.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "silex/silex": "^2.2",
        "symfony/assetic-bundle": "^2.8",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/symfony": "3.4.*",
        "mpdf/mpdf" : "^7.1",
        "twig/twig": "^1.0||^2.0",
        "willdurand/js-translation-bundle": "^2.6"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/browser-kit": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "EUREKA\\G6KBundle\\Composer\\ScriptHandler::installUsers",
            "EUREKA\\G6KBundle\\Composer\\ScriptHandler::installDemo"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "5.6"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "calcul",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
